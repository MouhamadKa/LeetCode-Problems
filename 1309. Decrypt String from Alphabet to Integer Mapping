# Solution on LeetCode: https://leetcode.com/problems/decrypt-string-from-alphabet-to-integer-mapping/solutions/7025785/very-simple-efficient-string-processing-oeei6/

class Solution:
    def freqAlphabets(self, s: str) -> str:
        res = ''
        i, length = 0, len(s)
        while i < length:
            if i < length - 2 and s[i + 2] == '#':
                res += chr(ord('j') + int(s[i: i + 2]) - 10)
                i += 3
            else:
                res += chr(ord('a') + int(s[i]) - 1)
                i += 1
        return res
